{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./",
    "allowJs": false,
    "noEmit": false,
    "incremental": true,
    "paths": {
      "@/vdb/*": ["../../node_modules/@vendure/dashboard/src/lib/*"]
    },
    "types": ["vitest/globals", "node"]
  },
  "exclude": ["**/node_modules", "**/.*/", "dist", "build", "_release"]
}
